<%= form_with model: @post, url:user_posts_path,method: Post do |form| %>

     <div class="new-post">
    <h3 class="new-post-header">Welcome to this platform</h3>
    <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :text %>
    <%= form.text_area :text %>
  </div>
    <div>
    <%= form.submit "Create Post" %>
  </div>
<%end%>

